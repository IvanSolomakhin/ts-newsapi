{
	"defaultSeverity": "error",
	"rulesDirectory": [
		  "node_modules/tslint-eslint-rules/dist/rules"
	  ],
	"rules": {
	  "align": [
		true,
		"arguments",
		"elements",
		"members",
		"parameters",
		"statements"
	  ],
	  "arrow-parens": {
		"options": "ban-single-arg-parens"
	  },
	  "indent": [
		true,
		"tabs",
		4
	  ],
	  "eofline": true,
	  "import-spacing": true,
	  "max-line-length": {
		"options": [
		  140
		]
	  },
	  "no-consecutive-blank-lines": false,
	  "no-console": {
		"severity": "warning",
		"options": [
		  "debug",
		  "info",
		  "log",
		  "time",
		  "timeEnd",
		  "trace"
		]
	  },
	  "ordered-imports": false,
	  "object-literal-sort-keys": false,
	  "semicolon": [
		true,
		"always",
		"ignore-bound-class-methods"
	  ],
	  "trailing-comma": true,
	  "triple-equals": true,
	  "curly": {
		"options": "ignore-same-line"
	  },
	  "one-variable-per-declaration": true,
	  "typedef-whitespace": {
		"options": {
		  "property-declaration": "nospace"
		}
	  },
	  "quotemark": [
		true,
		"single"
	  ],
	  "whitespace": [
		true,
		"check-branch",
		"check-decl",
		"check-operator",
		"check-separator",
		"check-type",
		"check-module",
		"check-rest-spread",
		"check-typecast",
		"check-type-operator",
		"check-preblock",
		"check-postbrace"
	  ],
	  "adjacent-overload-signatures": true,
	  "array-type": {
		"options": [
		  "generic"
		]
	  },
	  "callable-types": true,
	  "class-name": true,
	  "comment-format": {
		"options": [
		  "check-space"
		]
	  },
	  "cyclomatic-complexity": false,
	  "forin": true,
	  "jsdoc-format": true,
	  "label-position": true,
	  "max-classes-per-file": {
		"options": 1
	  },
	  "new-parens": true,
	  "no-angle-bracket-type-assertion": true,
	  "no-any": false,
	  "no-arg": true,
	  "no-bitwise": false,
	  "no-conditional-assignment": true,
	  "no-construct": true,
	  "no-debugger": true,
	  "no-duplicate-super": true,
	  "no-empty": true,
	  "no-empty-interface": true,
	  "no-eval": true,
	  "no-internal-module": true,
	  "no-invalid-this": true,
	  "no-misused-new": true,
	  "no-namespace": true,
	  "no-parameter-properties": false,
	  "no-reference": true,
	  "no-reference-import": true,
	  "no-shadowed-variable": true,
	  "no-string-literal": true,
	  "no-string-throw": true,
	  "no-switch-case-fall-through": false,
	  "no-trailing-whitespace": true,
	  "no-unnecessary-initializer": true,
	  "no-unsafe-finally": true,
	  "no-unused-expression": true,
	  "no-use-before-declare": false,
	  "no-var-keyword": true,
	  "no-var-requires": true,
	  "object-literal-shorthand": true,
	  "only-arrow-functions": {
		"options": [
		  "allow-declarations",
		  "allow-named-functions"
		]
	  },
	  "prefer-const": true,
	  "prefer-for-of": false,
	  "radix": true,
	  "typedef": false,
	  "unified-signatures": true,
	  "use-isnan": true,
	  "variable-name": {
		"options": [
		  "allow-leading-underscore",
		  "ban-keywords",
		  "check-format",
		  "allow-pascal-case"
		]
	  },
		  "array-bracket-spacing": [
			  true,
			  "always"
		  ],
		  "block-spacing": [
			  true,
			  "always"
		  ],		
		  "no-duplicate-variable": true,
		  "no-irregular-whitespace": true,
		  "switch-default": true,
		  "no-duplicate-imports": true,
		  "arrow-return-shorthand": true,
		  "ter-no-mixed-spaces-and-tabs": [
			  true,
			  "tabs"
		  ],
		  "space-before-function-paren": [true, {"anonymous": "always", "named": "never", "asyncArrow": "always"}],
		  "interface-name": true,
		  "no-inferrable-types": false,
		  "member-access": true,
		  "member-ordering": [
			  false
		  ],
		  "await-promise": true,
	  "no-floating-promises": true
	}
  }
  